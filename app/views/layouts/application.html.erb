<!DOCTYPE html>
<html>
<head>
  <title>効果測定自動解答ロボ</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div align="center">
<h1><a href="/">効果測定自動解答ロボ</a></h1>
<p><a href="/"><%= image_tag "title#{@result_pn}.png" %></a></p>

<%= form_tag('/analyze') do %>
  <%= text_area 'input','user', :size=>'60x3' %>
  <%= submit_tag '教えて! ロボ' %>
<% end %>

<%= yield %>
</div>
</body>
</html>
