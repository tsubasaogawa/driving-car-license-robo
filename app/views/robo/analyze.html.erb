
<p>
<%= @text %>
</p>

<p>
<% if @morpho_result %>
  <table class="table table-striped table-hover" width="300px">
    <caption>result by morphological analyzer using bag-of-words</caption>
    <thead>
      <tr><th>dict_id</th><th>word</th><th>count</th></tr>
    </thead>
    <tbody>
      <% @morpho_result.each do |e| %>
        <tr><td><%= e['id'] %></td><td><%= e['word'] %></td><td><%= e['count'] %></td></tr>
      <% end %>
    </tbody>
  </table>
<% end %>
</p>

<p>
system result: <%= @system_output %>
</p>
